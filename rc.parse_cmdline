#!/bin/sh

# (c) 2006 Krisztian VASAS <iron@frugalware.org>
# rc.parse_cmdline for FwLive
# distributed under GPL License

. /etc/rc.d/rc.functions

# description: Parses the parameters given in boot loader's command line

case $1 in
	start)
		start "$startparse"
		/usr/local/bin/parse_cmdline
		ok $?
	;;
	stop)
		/bin/true
	;;
esac
