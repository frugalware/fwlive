diff -Naur etc.old/rc.d/rc.dbus etc/rc.d/rc.dbus
--- etc.old/rc.d/rc.dbus	2006-06-26 00:40:35.000000000 +0200
+++ etc/rc.d/rc.dbus	2006-07-30 19:30:48.000000000 +0200
@@ -34,7 +34,7 @@
 	[ -d /usr/share/dbus-1/services ] || mkdir -p /usr/share/dbus-1/services
 	## lets kill this weird error if you try to start twice
