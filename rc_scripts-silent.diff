diff -Naur etc.old/rc.d/rc.modules etc/rc.d/rc.modules
--- etc.old/rc.d/rc.modules	2006-07-30 19:05:15.000000000 +0200
+++ etc/rc.d/rc.modules	2006-07-30 19:32:11.000000000 +0200
@@ -40,7 +40,7 @@
 		# only rebuild dependencies if new module(s) are found:
 		if ! [ "" = "$news" ]; then
 			start "$updatemoddeps"
-			/sbin/depmod -a
+			/sbin/depmod -a >/dev/null 2>&1
 			ok $?
 		else
 			start "$noupdate"
@@ -50,7 +50,7 @@
 		# we don't have find, or there is no existing modules.dep,
 		# or it is out of date.
 		start "$updatemoddeps"
-		/sbin/depmod -A
+		/sbin/depmod -A >/dev/null 2>&1
 		ok $?
 	fi
 
@@ -59,7 +59,7 @@
 	## At here you can do anything, run any commands, example iptables or any other base
 	## commands like in the older rc.modules file.
 	if grep -q quota /etc/fstab ; then
-	    $MODPROBEBIN quota_v2
+	    $MODPROBEBIN quota_v2 >/dev/null 2>&1
 	fi
 
 	## Starting reading conf file and loading modules
@@ -81,7 +81,7 @@
   				## If yes:  			
   				arg=$(echo $module | cut -f2 -d '"')
   				module=$(echo $module | cut -f1 -d '"')
-  				$MODPROBEBIN $module $arg
